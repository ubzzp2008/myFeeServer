<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper  namespace="com.jnc.web.dao.userinfo.IEasyPoiDao">

	<resultMap type="easyPoiDemo" id="demoResult">
			<result	property="mandt"	column="mandt"	/>
			<result	property="boxflag"	column="boxflag"	/>
			<result	property="matnr"	column="matnr"	/>
			<result	property="vtweg"	column="vtweg"	/>
			<result	property="uidstr"	column="uidstr"	/>
	</resultMap>

    <select id="queryAll" resultMap="demoResult"  parameterType="easyPoiDemo">
        select uidstr,
        mandt,
        boxflag,
        matnr,
        vtweg
        from
            bsap.zzp_wlm
        <where>
            <if test="flag!=null">
                and flag = #{flag}
            </if>
			<!--<if test="kunnr!=null">
				and kunnr = #{kunnr}
			</if>-->
            <if test="vtweg!=null">
                and vtweg = #{vtweg}
            </if>
			<if test="matnr!=null">
				and matnr = #{matnr}
			</if>
		</where>
    </select>
</mapper>